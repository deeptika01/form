#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() 
{
    int hh, mm, ss;
    scanf("%d: %d: %d", &hh, &mm, &ss);
    if (hh >= 0 && hh <= 23 && mm >= 0 && mm <= 59 && ss >= 0 && ss <= 59)
        printf("Valid\n");
    else
        printf("Not Valid\n");
    

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    return 0;
}
